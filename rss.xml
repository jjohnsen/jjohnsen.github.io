        srcset=&quot;/static/a345ffb063a399289d34b62e836d99a8/c26ae/ea-application-id.png 158w,
/static/a345ffb063a399289d34b62e836d99a8/6bdcf/ea-application-id.png 315w,
/static/a345ffb063a399289d34b62e836d99a8/f058b/ea-application-id.png 630w,
/static/a345ffb063a399289d34b62e836d99a8/cab43/ea-application-id.png 755w&quot;
        sizes=&quot;(max-width: 630px) 100vw, 630px&quot;
